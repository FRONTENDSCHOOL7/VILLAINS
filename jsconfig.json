{
    "compilerOptions": {
      "jsx": "react",
      "baseUrl": ".",
      "paths": {
        "@lib/*": ["../lib/*"],
        "@src/*": ["../*"],
        "@assets/*": ["../../assets/*"]
         },
      "module": "ESNext",
      "moduleResolution": "node"
    },
    "include": ["src", "**/*.js", "**/*.jsx"],
    "exclude": ["node_modules"]
  }